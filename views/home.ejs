<%- include('common/header');-%>
<link rel="stylesheet" href="../assets/style.css">
<div id="main-content">
    <h2>All Records</h2>
    <table cellpadding="7px">
        <thead>
        <th>Id</th>
        <th>Name</th>
        <th>Address</th>
        <th>Class</th>
        <th>Phone</th>
        <th>Action</th>
        </thead>
		
		
        <tbody>
            <% items.forEach(function(item){%>
            <tr>
                <td><%- item.sid %></td>
                <td><%- item.sname %></td>
                <td><%- item.saddress %></td>
                <td><%- item.cname %></td> 
                <td><%- item.sphone %></td>
                <td>
                    <a href='/edit?id=<%= item.sid %>'>Edit</a>
                    <a href='/delete?id=<%= item.sid %>' onclick='return confirm("Are you sure")'>Delete</a>
                </td>
            </tr>
            <% }) %>
        </tbody>
    </table>
    
</div>
</div>
</body>
</html>

